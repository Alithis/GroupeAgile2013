@using MyResources = ViewRes.AfterRegisterResource;
@{
    ViewBag.Registrationcomplete = MyResources.Registrationcomplete;
    ViewBag.PleaseFollowInstructions = MyResources.PleaseFollowInstructions;
    ViewBag.emailHasBeenSent = MyResources.emailHasBeenSent;
}
<section class="featured">
    <div class="row-fluid">
        <div class="span6 offset3 background_div">
            <div class="row-fluid">
                <div class="span12">
                    <div class="content-wrapper text_justified">
                        <hgroup class="title">
                            <h2>@ViewBag.Registrationcomplete</h2>
                            <h4>@ViewBag.PleaseFollowInstructions</h4>
                        </hgroup>
                        <p>
                            @ViewBag.emailHasBeenSent
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
